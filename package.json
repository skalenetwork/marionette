{
  "name": "marionette",
  "version": "0.0.1",
  "description": "Predeployed smart contract on skale chain that is controlled by external entity.",
  "main": "index.js",
  "repository": "git@github.com:skalenetwork/marionette.git",
  "author": "Dmytro Stebaiev <dmytro@skalelabs.com>",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "compile": "npx hardhat clean && npx hardhat compile",
    "prepare": "yarn compile && yarn hooks",
    "lint": "npx solhint \"contracts/**/*.sol\"",
    "cspell": "npx cspell \"**/*\"",
    "slither": "slither .",
    "tsc": "npx tsc --noEmit",
    "tslint": "npx tslint --project .",
    "test": "yarn tsc && npx hardhat test",
    "fullCheck": "yarn lint && yarn cspell && yarn slither && yarn tsc && yarn tslint",
    "hooks": "git config core.hooksPath .githooks || true",
    "no-hooks": "git config core.hooksPath .git/hooks"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cspell": "^5.13.1",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.7.0",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typechain": "^6.0.5",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "^4.3.3",
    "@skalenetwork/ima-interfaces": "^0.0.1"
  }
}
